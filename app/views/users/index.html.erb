<div class="container">
  <h2>参加者一覧</h2>
  <% @users.each do |user| %>
    <div class="container-child col-md-2 col-sm-4 col-xs-12 ">
      <div class= "img-responsive">
        <%= profile_img(user) %>
      </div>
      <br>
        <%= user.email %>
      <br>
        <%= user.name %>
      <br>
        <%= render 'follow_form', user: user %>
    </div>
  <% end %>
</div>
