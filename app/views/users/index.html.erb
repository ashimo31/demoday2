<div class="container">
  <h2>ユーザー一覧</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(user) %>
        <br>
        <%= user.email %>
        <br>
        <%= user.name %>
        <br>
        <%= render 'follow_form', user: user %>
      </div>
  <% end %>
</div>
